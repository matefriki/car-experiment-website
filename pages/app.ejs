<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" href="/public/css/style.css">
        <link rel="stylesheet" href="/css/bootstrap.min.css">
        <link rel="stylesheet" href="/public/css/dropdown.css">

        <script src="/js/bootstrap.min.js"></script>
        <script src="/pixi/pixi.js"></script>
        <script src="/socket.io/socket.io.js"></script>
        <script src="/public/js/dropdown.js"></script>
        <script src="/script.js"></script>

        <title>Car Experiment - Simulation</title>
    </head>

    <body>
        <div id="page-container">
            <div id="content-wrap">
                <%- include('../partials/header'); %>
                <div class="app">
                    <div class="controls">
                        <div class="outline">
                            <div class="pane person">
                                <div class="title">Person</div>
                                <div class="property">
                                    <span class="label">X: </span>
                                    <div class="input editable" data-obj="person" data-prop="x" data-range="25:75" contenteditable="true">50</div>
                                </div>
                                <div class="property">
                                    <span class="label">Y: </span>
                                    <div class="input editable" data-obj="person" data-prop="y" data-range="0:15" contenteditable="true">1</div>
                                </div>
                                <div class="button unselectable random">Randomize</div>
                            </div>
                            <div class="pane car">
                                <div class="title">Car</div>
                                <div class="property">
                                    <span class="label">X: </span>
                                    <div class="input editable randomizable" data-obj="car" data-prop="x" data-range="25:75" contenteditable="true">27</div>
                                </div>
                                <div class="property">
                                    <span class="label">Y: </span>
                                    <div class="input editable randomizable" data-obj="car" data-prop="y" data-range="5:5" contenteditable="true">5</div>
                                </div>
                                <div class="property car-velocity">
                                    <span class="label">V: </span>
                                    <div class="input" data-range="0:3">10</div>
                                </div>
                                <div class="button unselectable random">Randomize</div>
                                <div class="strat-dropdown dropdown-component" data-options="cautious,normal,agressive,car1,car2,car3,car4"></div>
                            </div>
                            <div class="pane block">
                                <div class="title">Block</div>
                                <div class="property">
                                    <span class="label">X1: </span>
                                    <div class="input editable" data-obj="bottom_corner" data-prop="x" data-range="25:75" contenteditable="true">56</div>
                                </div>
                                <div class="property">
                                    <span class="label">Y1: </span>
                                    <div class="input editable" data-obj="bottom_corner" data-prop="y" data-range="0:5" contenteditable="true">2</div>
                                </div>
                                <div class="property">
                                    <span class="label">X2: </span>
                                    <div class="input editable" data-obj="top_corner" data-prop="x" data-range="25:75" contenteditable="true">62</div>
                                </div>
                                <div class="property">
                                    <span class="label">Y2: </span>
                                    <div class="input editable" data-obj="top_corner" data-prop="y" data-range="0:5" contenteditable="true">4</div>
                                </div>
                            </div>
                            <div class="generate">
                                <div class="label">Generate</div>
                                <div class="input" data-range="1:300" contenteditable="true">100</div>
                                <div class="label">steps</div>
                                <div class="big_button unselectable">go!</div>
                            </div>
                            <div class="loading">
                                <div class="progress">
                                    <div class="bar"></div>
                                </div>
                            </div>
                            <div class="replay-pane">
                                <div class="replay disabled">
                                    <div class="replay-inner">
                                        <img src="/assets/replay.png" alt="">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <canvas class="scene"></canvas>
                    <div class="bottom">
                        <img src="/assets/spin.png" alt="" class="spinner">
                        <div class="graph-container">
                            <canvas class="graph_left"></canvas>
                            <canvas class="graph_right"></canvas>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>